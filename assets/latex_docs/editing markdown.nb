Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"stc", "=", 
    RowBox[{
     RowBox[{"StringPosition", "[", 
      RowBox[{"e", ",", "\"\<[@\>\""}], "]"}], "[", 
     RowBox[{"[", 
      RowBox[{"All", ",", "1"}], "]"}], "]"}]}], ";"}], " ", 
  RowBox[{"(*", 
   RowBox[{
   "This", " ", "finds", " ", "the", " ", "place", " ", "citations", " ", 
    "of", " ", "the", " ", 
    RowBox[{"form", " ", "[", 
     RowBox[{"@", "reference"}], "]"}], " ", "start"}], 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"stpe", "=", 
    RowBox[{
     RowBox[{"StringPosition", "[", 
      RowBox[{"e", ",", "\"\<]\>\""}], "]"}], "[", 
     RowBox[{"[", 
      RowBox[{"All", ",", "2"}], "]"}], "]"}]}], ";"}], 
  RowBox[{"(*", 
   RowBox[{
   "This", " ", "finds", " ", "all", " ", "the", " ", "places", " ", 
    "citations", " ", "could", " ", "end"}], "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"find", "[", "x_", "]"}], ":=", 
    RowBox[{"Min", "[", 
     RowBox[{"Select", "[", 
      RowBox[{"stpe", ",", " ", 
       RowBox[{
        RowBox[{"#", ">", "x"}], "&"}]}], "]"}], "]"}]}], " ", ";"}], 
  RowBox[{"(*", 
   RowBox[{
   "Function", " ", "that", " ", "given", " ", "an", " ", "element", " ", 
    "of", " ", "the", " ", "form", " ", "x", " ", "finds", " ", "min", " ", 
    "of", " ", "stpe", " ", "bigger", " ", "than", " ", "x"}], "*)"}]}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"ste", "=", 
    RowBox[{"Map", "[", 
     RowBox[{"find", ",", "stc"}], "]"}]}], ";"}], " ", 
  RowBox[{"(*", 
   RowBox[{
   "This", " ", "finds", " ", "the", " ", "place", " ", "citations", " ", 
    "of", " ", "the", " ", 
    RowBox[{"form", " ", "[", 
     RowBox[{"@", "reference"}], "]"}], " ", "start"}], "*)"}]}], "\n", 
 RowBox[{
  RowBox[{"Map", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"StringTake", "[", 
      RowBox[{"e", ",", "#"}], "]"}], "&"}], ",", 
    RowBox[{"Transpose", "[", 
     RowBox[{"{", 
      RowBox[{"stc", ",", "ste"}], "}"}], "]"}]}], "]"}], " ", 
  RowBox[{"(*", 
   RowBox[{"This", " ", "obtains", " ", "all", " ", "the", " ", "full", " ", 
    RowBox[{"references", " ", "[", 
     RowBox[{"@", "reference"}], "]"}]}], "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{"citations", "=", 
  RowBox[{"Map", "[", 
   RowBox[{
    RowBox[{
